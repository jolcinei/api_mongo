<div class="author">
    <h1 class="author__heading">Resultado</h1>
</div>

<section class="articles">
    <ul class="article-ul">
        <% posts.forEach(post=> { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <span>
                        <%= post.title %>
                    </span>
                    <span class="article-list__date">
                        <%= post.createdAt.toDateString() %>
                    </span>
                </a>
            </li>
            <% }) %>

    </ul>
</section>